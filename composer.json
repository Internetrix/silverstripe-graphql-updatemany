{
  "name": "internetrix/silverstripe-graphql-updatemany",
  "description": "Adds graphql support for an updateMany scaffolder",
  "type": "silverstripe-vendormodule",
  "homepage": "https://github.com/Internetrix/silverstripe-graphql-updatemany",
  "keywords": [
    "silverstripe",
    "module",
    "graphql",
    "update",
    "updateMany",
    "scaffolders"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Guy Watson",
      "email": "guy.watson@internetrix.com.au"
    }
  ],
  "support": {
    "issues": "https://github.com/Internetrix/silverstripe-graphql-updatemany/issues"
  },
  "require": {
    "silverstripe/vendor-plugin": "^1.0",
    "silverstripe/framework": "~4.0",
    "silverstripe/graphql": "~1.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "extra": {
    "installer-name": "graphql-updatemany"
  },
  "autoload": {
    "psr-4": {
      "Internetrix\\GraphQLUpdateMany\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
